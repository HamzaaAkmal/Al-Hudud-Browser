<?xml version="1.0" encoding="utf-8"?>
<!-- Configuration for Security Protection Accessibility Service -->
<accessibility-service xmlns:android="http://schemas.android.com/apk/res/android"
    android:description="@string/security_protection_accessibility_service_description"
    android:accessibilityEventTypes="typeWindowStateChanged|typeViewClicked|typeWindowContentChanged"
